@model OrderModel
@{
    ViewData["Title"] = "PayUpOverview";
}

@if (TempData["message"] != null)
{
    <div class="alert alert-warning"> @TempData["message"]</div>
}
<div class="overviewConstraint">
    @await Html.PartialAsync("DishPayUp", Model)
</div>
<form id="saveOrder" asp-action="PublishOrder" asp-controller="Order">
    <button class="btn btn-info" type="submit">Save</button>
</form>

